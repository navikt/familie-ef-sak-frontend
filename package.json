{
    "name": "familie-ef-sak-frontend",
    "version": "0.0.1",
    "main": "node_dist/server.js",
    "repository": "https://github.com/navikt/familie-ef-sak-frontend.git",
    "author": "Nav - teamfamilie",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "yarn set-node-options NODE_ENV=production node node_dist/backend/server.js",
        "start:mock": "yarn set-node-options NODE_ENV=development node dev-server/mock-server.js",
        "start:dev": "tsc && yarn set-node-options NODE_ENV=development node --trace-deprecation node_dist/backend/server-lokal.js",
        "build": "yarn lint && yarn build:prod",
        "build:dev": "tsc & NODE_ENV=development webpack --config ./build_n_deploy/webpack/webpack.dev.js",
        "build:prod": "tsc & NODE_ENV=production webpack --config ./build_n_deploy/webpack/webpack.prod.js",
        "lint": "tsc --project ./src/frontend/tsconfig.json --noEmit && eslint './src/**/*.{js,ts,tsx}'",
        "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
        "test": "vitest run --exclude '**/frontend_production/**'",
        "prepare": "husky install",
        "set-node-options": "NODE_OPTIONS='--import=./node_dist/backend/register.js --es-module-specifier-resolution=node'"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css}": ["prettier --write", "eslint --fix --max-warnings=0"]
    },
    "dependencyComments": {
        "color-string": "Overrider versjon som brukes av winston, som kommer fra familie-frontend-backend",
        "body-parser": "Overrider versjon som  kommer fra familie-frontend backend"
