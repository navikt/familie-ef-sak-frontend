    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "yarn set-node-options NODE_ENV=production node node_dist/backend/server.js",
        "start:mock": "yarn set-node-options NODE_ENV=development node dev-server/mock-server.js",
        "start:dev": "tsc && yarn tsc-alias && yarn set-node-options NODE_ENV=development node --trace-deprecation node_dist/backend/server-lokal.js",
        "build": "yarn lint && yarn build:prod && yarn tsc-alias",
        "build:dev": "tsc & NODE_ENV=development webpack --config ./webpack/webpack.dev.js",
        "build:prod": "tsc & NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
        "lint": "tsc --project ./src/frontend/tsconfig.json --noEmit && eslint './src/**/*.{js,ts,tsx}'",
        "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
        "test": "vitest run --exclude '**/frontend_production/**'",
        "prepare": "husky",
        "set-node-options": "NODE_OPTIONS='--es-module-specifier-resolution=node'",
        "tsc-alias": "tsc-alias -p tsconfig.json"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json}": ["prettier --write", "eslint --fix"]
    },
    "dependencyComments": {
        "color-string": "Overrider versjon som brukes av winston, som kommer fra familie-frontend-backend",
