{
    "printWidth": 100,
    "parser": "typescript",
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "overrides": [
        {
            "files": "*.md",
            "options": {
                "parser": "markdown"
            }
        },
        {
            "files": "*.yaml",
            "options": {
                "parser": "yaml"
            }
        },
        {
            "files": "*.yml",
            "options": {
                "parser": "yaml"
            }
        },
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        }
    ]
}